{
  "name": "@habityzer/nuxt-openapi-composables",
  "version": "1.4.1",
  "description": "Generate type-safe Nuxt 3 & 4 composables from OpenAPI schemas",
  "author": "Vazgen Manukyan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Habityzer/nuxt-openapi-composables.git"
  },
  "homepage": "https://github.com/Habityzer/nuxt-openapi-composables#readme",
  "bugs": {
    "url": "https://github.com/Habityzer/nuxt-openapi-composables/issues"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./runtime": {
      "types": "./dist/runtime/useApi.d.ts",
      "import": "./dist/runtime/useApi.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "nuxt-openapi-composables": "./dist/cli/index.js",
    "habityzer-openapi": "./dist/cli/index.js"
  },
  "files": [
    "dist",
    "templates",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "pnpm build && pnpm test",
    "release": "pnpm build && pnpm test && HUSKY=0 semantic-release"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "consola": "^3.2.3",
    "openapi-typescript": "^7.9.1",
    "pathe": "^1.1.2",
    "picocolors": "^1.1.1"
  },
  "peerDependencies": {
    "nuxt": "^3.0.0 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "nuxt": {
      "optional": true
    }
  },
  "devDependencies": {
    "@nuxt/kit": "^3.14.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "semantic-release": "^24.2.9",
    "tsup": "^8.0.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "nuxt",
    "nuxt3",
    "nuxt4",
    "openapi",
    "composables",
    "api",
    "generator",
    "typescript",
    "code-generation"
  ],
  "packageManager": "pnpm@10.18.1",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
